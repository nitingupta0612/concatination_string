#include<iostream>
#include<string.h>
#include<conio.h>
using namespace std;
class String
{
	char str[200];
	public:
		void read()
		{
			cout<<"Enter a string: ";
			cin>>str;
		}
	void print()
	{
		cout<<"String is : "<<str<<endl;
	}
	String operator + (String x)
	{
		String t;
		strcpy(t.str,str);
		strcat(t.str,x.str);
		return t;
	}
};
int main()
{
	String s1,s2,s3;
	s1.read();
	s2.read();
	s3=s1+s2;
	s3.print();
	return 0;
}
