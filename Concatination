#include<iostream>
#include<string.h>
#include<math.h>
using namespace std;
class String
{
	char str[200];
	public:
		void readdata()
		{
			cout<<"Enter a string: ";
			cin>>str;
		}
	void print()
	{
		cout<<"String is : "<<str<<endl;
	}
	String operator + (String x)
	{
		String t;
		strcpy(t.str,str);
		strcat(t.str,x.str);
		return t;
	}
};
int main()
{
	String str1,str2,str3;
	str1.readdata();
	str2.readdata();
	str3=str1+str2;
	str3.print();
	return 0;
}
