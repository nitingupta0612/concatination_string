#include<iostream>
#include<string.h>
using namespace std;
class STRING
{
	char str[200];
	public:
		void read()
		{
			cout<<"Enter String: ";
			cin>>str;
		}
	void print()
	{
		cout<<"String is : "<<str<<endl;
	}
	STRING operator + (STRING x)
	{
		STRING t;
		strcpy(t.str,str);
		strcat(t.str,x.str);
		return t;
	}
};
int main()
{
	STRING s1,s2,s3;
	s1.read();
	s2.read();
	s3=s1+s2;
	s3.print();
	return 0;
}
